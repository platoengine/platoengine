<?xml version="1.0"?>

<Console>
  <Enabled>true</Enabled>
  <Verbose>true</Verbose>
</Console>

<include filename="defines.xml"/>

<Console>
  <Verbose>true</Verbose>
</Console>

<!-- Define Performers -->

<Performer>
  <Name>PlatoMain</Name>
  <Code>PlatoMain</Code>
  <PerformerID>0</PerformerID>
</Performer>
<Performer>
  <For var="I" in="Parameters">
  <Name>PlatoESP_{I}</Name>
  </For>
  <PerformerID>1</PerformerID>
</Performer>
<Performer>
  <Name>plato_analyze_1</Name>
  <PerformerID>2</PerformerID>
</Performer>

<!-- Define SharedData -->

<SharedData>
  <Name>Objective 1</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>plato_analyze_1</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>

<For var="I" in="Parameters">
<SharedData>
  <Name>Parameter Sensitivity {I}</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Dynamic>true</Dynamic>
  <OwnerName>PlatoESP_{I}</OwnerName>
  <UserName>plato_analyze_1</UserName>
</SharedData>
</For>
<SharedData>
  <Name>Objective 1 Gradient</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>plato_analyze_1</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Objective</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Objective Gradient</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Design Parameters</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
  <UserName>plato_analyze_1</UserName>
</SharedData>
<SharedData>
  <Name>Constraint</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>plato_analyze_1</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Constraint Gradient</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>plato_analyze_1</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Lower Bound Value</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Upper Bound Value</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>1</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Lower Bound Vector</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>
<SharedData>
  <Name>Upper Bound Vector</Name>
  <Type>Scalar</Type>
  <Layout>Global</Layout>
  <Size>5</Size>
  <OwnerName>PlatoMain</OwnerName>
  <UserName>PlatoMain</UserName>
</SharedData>


<!-- Define Stages -->


<Stage>
  <Name>Output To File</Name>
  <Operation>
    <Name>CSMMeshOutput</Name>
    <PerformerName>PlatoMain</PerformerName>
  </Operation>
</Stage>
<Stage>
  <Name>Initialize Design Parameters</Name>
  <Operation>
    <Name>Initialize Values</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Output>
      <ArgumentName>Values</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Output>
    <Output>
      <ArgumentName>Lower Bounds</ArgumentName>
      <SharedDataName>Lower Bound Vector</SharedDataName>
    </Output>
    <Output>
      <ArgumentName>Upper Bounds</ArgumentName>
      <SharedDataName>Upper Bound Vector</SharedDataName>
    </Output>
  </Operation>
  <Operation>
    <Name>Initialize Geometry</Name>
    <PerformerName>PlatoMain</PerformerName>
  </Operation>
  <Output>
    <SharedDataName>Design Parameters</SharedDataName>
  </Output>
</Stage>
<Stage>
  <Name>Cache State</Name>
</Stage>
<Stage>
  <Name>Set Lower Bounds</Name>
  <Output>
    <SharedDataName>Lower Bound Vector</SharedDataName>
  </Output>
</Stage>
<Stage>
  <Name>Set Upper Bounds</Name>
  <Output>
    <SharedDataName>Upper Bound Vector</SharedDataName>
  </Output>
</Stage>
<Stage>
  <Name>Constraint</Name>
  <Input>
    <SharedDataName>Design Parameters</SharedDataName>
  </Input>
  <Operation>
    <Name>Update Geometry on Change</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Reinitialize on Change</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Compute Constraint Value</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Output>
      <ArgumentName>Constraint Value</ArgumentName>
      <SharedDataName>Constraint</SharedDataName>
    </Output>
  </Operation>
  <Output>
    <SharedDataName>Constraint</SharedDataName>
  </Output>
</Stage>
<Stage>
  <Name>Constraint Gradient</Name>
  <Input>
    <SharedDataName>Design Parameters</SharedDataName>
  </Input>
  <Operation>
    <Name>Update Geometry on Change</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Reinitialize on Change</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Operation>
      <Name>Compute Constraint Gradient</Name>
      <PerformerName>plato_analyze_1</PerformerName>
      <!-- no output -->
    </Operation>
    <For var="I" in="Parameters">
    <Operation>
      <Name>Compute Shape Sensitivity on Change</Name>
      <PerformerName>PlatoESP_{I}</PerformerName>
      <Parameter>
        <ArgumentName>Parameter Index</ArgumentName>
        <ArgumentValue>{I-1}</ArgumentValue>
      </Parameter>
      <Input>
        <ArgumentName>Parameters</ArgumentName>
        <SharedDataName>Design Parameters</SharedDataName>
      </Input>
      <Output>
        <ArgumentName>Parameter Sensitivity</ArgumentName>
        <SharedDataName>Parameter Sensitivity {I}</SharedDataName>
      </Output>
    </Operation>
    </For>
  </Operation>
  <Operation>
    <Name>Compute Constraint Sensitivity</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <For var="I" in="Parameters">
    <Input>
      <ArgumentName>Parameter Sensitivity {I}</ArgumentName>
      <SharedDataName>Parameter Sensitivity {I}</SharedDataName>
    </Input>
    </For>
    <Output>
      <ArgumentName>Criterion Sensitivity</ArgumentName>
      <SharedDataName>Constraint Gradient</SharedDataName>
    </Output>
  </Operation>
  <Output>
    <SharedDataName>Constraint Gradient</SharedDataName>
  </Output>
</Stage>

<Stage>
  <Name>Objective</Name>
  <Input>
    <SharedDataName>Design Parameters</SharedDataName>
  </Input>
  <Operation>
    <Name>Update Geometry on Change</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Reinitialize on Change</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Compute Objective Value</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Output>
      <ArgumentName>Objective Value</ArgumentName>
      <SharedDataName>Objective 1</SharedDataName>
    </Output>
  </Operation>
  <Operation>
    <Name>AggregateValues</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Value 1</ArgumentName>
      <SharedDataName>Objective 1</SharedDataName>
    </Input>
    <Output>
      <ArgumentName>Values</ArgumentName>
      <SharedDataName>Objective</SharedDataName>
    </Output>
  </Operation>
  <Output>
    <SharedDataName>Objective</SharedDataName>
  </Output>
</Stage>

<Stage>
  <Name>Objective Gradient</Name>
  <Input>
    <SharedDataName>Design Parameters</SharedDataName>
  </Input>
  <Operation>
    <Name>Update Geometry on Change</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Name>Reinitialize on Change</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <Input>
      <ArgumentName>Parameters</ArgumentName>
      <SharedDataName>Design Parameters</SharedDataName>
    </Input>
  </Operation>
  <Operation>
    <Operation>
      <Name>Compute Objective Gradient</Name>
      <PerformerName>plato_analyze_1</PerformerName>
      <!-- no output -->
    </Operation>
    <For var="I" in="Parameters">
    <Operation>
      <Name>Compute Parameter Sensitivity on Change</Name>
      <PerformerName>PlatoESP_{I}</PerformerName>
      <Parameter>
        <ArgumentName>Parameter Index</ArgumentName>
        <ArgumentValue>{I-1}</ArgumentValue>
      </Parameter>
      <Input>
        <ArgumentName>Parameters</ArgumentName>
        <SharedDataName>Design Parameters</SharedDataName>
      </Input>
      <Output>
        <ArgumentName>Parameter Sensitivity</ArgumentName>
        <SharedDataName>Parameter Sensitivity {I}</SharedDataName>
      </Output>
    </Operation>
    </For>
  </Operation>
  <Operation>
    <Name>Compute Objective Sensitivity</Name>
    <PerformerName>plato_analyze_1</PerformerName>
    <For var="I" in="Parameters">
    <Input>
      <ArgumentName>Parameter Sensitivity {I}</ArgumentName>
      <SharedDataName>Parameter Sensitivity {I}</SharedDataName>
    </Input>
    </For>
    <Output>
      <ArgumentName>Criterion Sensitivity</ArgumentName>
      <SharedDataName>Objective 1 Gradient</SharedDataName>
    </Output>
  </Operation>
  <Operation>
    <Name>AggregateValues</Name>
    <PerformerName>PlatoMain</PerformerName>
    <Input>
      <ArgumentName>Value 1</ArgumentName>
      <SharedDataName>Objective 1 Gradient</SharedDataName>
    </Input>
    <Output>
      <ArgumentName>Values</ArgumentName>
      <SharedDataName>Objective Gradient</SharedDataName>
    </Output>
  </Operation>
  <Output>
    <SharedDataName>Objective Gradient</SharedDataName>
  </Output>
</Stage>
<Optimizer>
  <Package>MMA</Package>
  <Options>
    <MaxNumOuterIterations>2</MaxNumOuterIterations>
    <MoveLimit>0.5</MoveLimit>
    <AsymptoteExpansion>1.2</AsymptoteExpansion>
    <AsymptoteContraction>0.7</AsymptoteContraction>
    <MaxNumSubProblemIter>50</MaxNumSubProblemIter>
    <ControlStagnationTolerance>-1</ControlStagnationTolerance>
    <ObjectiveStagnationTolerance>-1</ObjectiveStagnationTolerance>
  </Options>
  <Output>
    <OutputStage>Output To File</OutputStage>
  </Output>
  <CacheStage>
    <Name>Cache State</Name>
  </CacheStage>
  <OptimizationVariables>
    <ValueName>Design Parameters</ValueName>
    <InitializationStage>Initialize Design Parameters</InitializationStage>
    <FilteredName>Topology</FilteredName>
    <LowerBoundValueName>Lower Bound Value</LowerBoundValueName>
    <LowerBoundVectorName>Lower Bound Vector</LowerBoundVectorName>
    <UpperBoundValueName>Upper Bound Value</UpperBoundValueName>
    <UpperBoundVectorName>Upper Bound Vector</UpperBoundVectorName>
    <SetLowerBoundsStage>Set Lower Bounds</SetLowerBoundsStage>
    <SetUpperBoundsStage>Set Upper Bounds</SetUpperBoundsStage>
  </OptimizationVariables>
  <Objective>
    <ValueName>Objective</ValueName>
    <ValueStageName>Objective</ValueStageName>
    <GradientName>Objective Gradient</GradientName>
    <GradientStageName>Objective Gradient</GradientStageName>
  </Objective>
  <BoundConstraint>
    <Upper>1.0</Upper>
    <Lower>0.0</Lower>
  </BoundConstraint>
  <Constraint>
    <ValueName>Constraint</ValueName>
    <ValueStageName>Constraint</ValueStageName>
    <GradientName>Constraint Gradient</GradientName>
    <GradientStageName>Constraint Gradient</GradientStageName>
    <AbsoluteTargetValue>17.5</AbsoluteTargetValue>
    <ReferenceValue>17.5</ReferenceValue>
  </Constraint>
</Optimizer>
